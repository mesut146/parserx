A: a? a b
A: () a b | a b
A: a{} a{} b{b} | a{} b{b}
A: a (a b | b)


A A(){
  A res;
  Token a = consume(a);
  switch(peek().type){
    case a:
     res.a = a;
     res.a2 = consume(a);
     res.b = consume(b);
    case b:
      res.a2 = a;
      res.b = consume(b);
  }
  return res;
}


class A{
  Token a;
  Token a2;
  Token b;
}
