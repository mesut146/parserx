table[state][symbol_id] = action:int

if(action > 0){
  //shift
  states.push(action);
  stack.push(symbol);
}
else{
  if(action == 0) error?
  //reduce
  int size = rhs_sizes[-action];
  while(size > 0){
    stack.pop();
    states.pop();
  }
  stack.push(new Symbol(-action));
  print();

}



